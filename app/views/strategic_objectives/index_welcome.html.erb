<%= content_for(:so_menu, &Proc.new { "selected" }) %>

<div class="inside-box max-inner-width">
	<div class="strategic-lines-img image"></div>
	<div class="text-content">
		<h1><%= t('views.strategic_objectives.index.empty.title') %></h1>
	
		<div class="contextual">
			<%= form_tag contextual_legends_show_path, :remote => true, :id => "contextual-show-action-form" do %>
				<%= hidden_field_tag :route, request.env['PATH_INFO'] %>
			<% end %>
		</div>
	</div>
	<br/>		
	<%= link_to t('views.strategic_objectives.index.empty.controls.start'), new_strategic_objective_url(with_current_subdomain), :class => "button orange start-button" %>
</div>
