<div class="box rounded-10 with-shadow with-mid-width">
	
	<div class="inside-box">
		<!-- Image space -->
		<h1><%= t('views.strategic_objectives.index.empty.title') %></h1>
		
		<div class="contextual">
			<%= form_tag contextual_legends_show_path, :remote => true, :id => "contextual-show-action-form" do %>
				<%= hidden_field_tag :route, request.env['PATH_INFO'] %>
			<% end %>
		</div>
		
		<br/>		
		<%= link_to t('views.strategic_objectives.index.empty.controls.start'), new_strategic_objective_url(with_current_subdomain), :class => "button orange" %>
	</div>
</div>
