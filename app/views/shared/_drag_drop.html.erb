<%- content_for :javascript do %>
	<%= javascript_include_tag 'jquery-ui.min','drag_drop' %>
	<script type="text/javascript">
	controller="operative_objectives";
	<%-unless params[:controller].include?("swot") %>
	model="<%=params[:controller].camelize.singularize%>";
	<%-else%>
		model="Analysis";
		$(document).ready(function() {dragAndDrop("drag2","");});
	<%-end%>
	if(model="Area"){
		model="AreaSupport";
	}
	</script>
	<%end%>